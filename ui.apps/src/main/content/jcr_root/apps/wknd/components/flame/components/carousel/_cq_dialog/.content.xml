<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
		  xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
		  xmlns:cq="http://www.day.com/jcr/cq/1.0"
		  xmlns:jcr="http://www.jcp.org/jcr/1.0"
		  xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
		  jcr:primaryType="nt:unstructured"
		  jcr:title="Carrossel - Flame"
		  sling:resourceType="cq/gui/components/authoring/dialog"
		  extraClientlibs="[cq.author.showhide]"
		  helpPath="/mnt/overlay/wcm/core/content/sites/components/details.html/apps/santander-spa-first/components/flame/components/carousel">
	<content
			jcr:primaryType="nt:unstructured"
			sling:resourceType="granite/ui/components/foundation/container">
		<items jcr:primaryType="nt:unstructured">
			<tabs
					jcr:primaryType="nt:unstructured"
					sling:resourceType="granite/ui/components/coral/foundation/tabs"
					maximized="{Boolean}true">
				<items jcr:primaryType="nt:unstructured">
					<carouselModel
							jcr:primaryType="nt:unstructured"
							jcr:title="Modelo de Carrossel"
							sling:resourceType="granite/ui/components/coral/foundation/container"
							margin="{Boolean}true">
						<items jcr:primaryType="nt:unstructured">
							<carouselModel
									jcr:primaryType="nt:unstructured"
									sling:resourceType="granite/ui/components/foundation/include"
									path="/apps/santander-spa-first/components/reusable/dialog/cq:dialog/content/items/carouselModel"/>
						</items>
					</carouselModel>
					<properties
							jcr:primaryType="nt:unstructured"
							jcr:title="Itens do Carrossel"
							sling:resourceType="granite/ui/components/coral/foundation/container"
							margin="{Boolean}true">
						<items jcr:primaryType="nt:unstructured">
							<multifield
									jcr:primaryType="nt:unstructured"
									sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
									composite="{Boolean}true"
									fieldLabel="Item Carrossel">
								<field
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/container"
										name="./carouselItems">
									<items jcr:primaryType="nt:unstructured">
										<item
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/accordion"
												composite="{Boolean}true"
												name="./item">
											<items jcr:primaryType="nt:unstructured">
												<item jcr:primaryType="nt:unstructured"
													  sling:resourceType="granite/ui/components/coral/foundation/well"
													  jcr:title="Item Carrossel">
													<items jcr:primaryType="nt:unstructured">
														<typography
																cq:showOnCreate="{Boolean}true"
																jcr:primaryType="nt:unstructured"
																jcr:title="Tipografia"
																sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
															<items jcr:primaryType="nt:unstructured">
																<primaryText
																		jcr:primaryType="nt:unstructured"
																		sling:resourceType="granite/ui/components/foundation/include"
																		path="/apps/santander-spa-first/components/reusable/dialog/cq:dialog/content/items/primaryText"/>
																<secondaryText
																		jcr:primaryType="nt:unstructured"
																		sling:resourceType="granite/ui/components/foundation/include"
																		path="/apps/santander-spa-first/components/reusable/dialog/cq:dialog/content/items/secondaryText"/>
																<tertiaryText
																		jcr:primaryType="nt:unstructured"
																		sling:resourceType="granite/ui/components/foundation/include"
																		path="/apps/santander-spa-first/components/reusable/dialog/cq:dialog/content/items/tertiaryText"/>
															</items>
														</typography>
														<image
																cq:showOnCreate="{Boolean}true"
																jcr:primaryType="nt:unstructured"
																jcr:title="Imagem"
																sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
															<items jcr:primaryType="nt:unstructured">
																<image
																		jcr:primaryType="nt:unstructured"
																		sling:resourceType="granite/ui/components/foundation/include"
																		path="/apps/santander-spa-first/components/reusable/dialog/cq:dialog/content/items/image"/>
																<mobileImage
																		jcr:primaryType="nt:unstructured"
																		sling:resourceType="granite/ui/components/foundation/include"
																		path="/apps/santander-spa-first/components/reusable/dialog/cq:dialog/content/items/mobileImage"/>
															</items>
														</image>
														<button
																cq:showOnCreate="{Boolean}true"
																jcr:primaryType="nt:unstructured"
																jcr:title="Botão"
																sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
															<items jcr:primaryType="nt:unstructured">
																<primaryButton
																		jcr:primaryType="nt:unstructured"
																		sling:resourceType="granite/ui/components/foundation/include"
																		path="/apps/santander-spa-first/components/reusable/dialog/cq:dialog/content/items/primaryButton"/>
															</items>
														</button>
														<backgroundOptions
																cq:showOnCreate="{Boolean}true"
																jcr:primaryType="nt:unstructured"
																jcr:title="Opções de Plano de Fundo"
																sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
															<items jcr:primaryType="nt:unstructured">
																<backgroundOptions
																		jcr:primaryType="nt:unstructured"
																		sling:resourceType="granite/ui/components/foundation/include"
																		path="/apps/santander-spa-first/components/reusable/dialog/cq:dialog/content/items/backgroundOptions"/>
															</items>
														</backgroundOptions>
													</items>
												</item>
											</items>
										</item>
									</items>
								</field>
							</multifield>
						</items>
					</properties>
				</items>
			</tabs>
		</items>
	</content>
</jcr:root>