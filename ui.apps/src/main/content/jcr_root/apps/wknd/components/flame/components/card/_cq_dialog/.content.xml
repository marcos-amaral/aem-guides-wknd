<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
		  xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
		  xmlns:cq="http://www.day.com/jcr/cq/1.0"
		  xmlns:jcr="http://www.jcp.org/jcr/1.0"
		  xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
		  jcr:primaryType="nt:unstructured"
		  jcr:title="Card - Flame"
		  sling:resourceType="cq/gui/components/authoring/dialog"
		  extraClientlibs="[cq.author.showhide]"
		  helpPath="/mnt/overlay/wcm/core/content/sites/components/details.html/apps/santander-spa-first/components/flame/components/carousel">
	<content
			jcr:primaryType="nt:unstructured"
			sling:resourceType="granite/ui/components/foundation/container">
		<items jcr:primaryType="nt:unstructured">
			<tabs
					jcr:primaryType="nt:unstructured"
					sling:resourceType="granite/ui/components/coral/foundation/tabs"
					maximized="{Boolean}true">
				<items jcr:primaryType="nt:unstructured">
					<properties
							jcr:primaryType="nt:unstructured"
							jcr:title="Cards"
							sling:resourceType="granite/ui/components/coral/foundation/container"
							margin="{Boolean}true">
						<items jcr:primaryType="nt:unstructured">
							<multifield
									jcr:primaryType="nt:unstructured"
									sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
									composite="{Boolean}true"
									fieldLabel="Cards">
								<field
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/container"
										name="./items">
									<items jcr:primaryType="nt:unstructured">
										<item
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/accordion"
												composite="{Boolean}true"
												name="./item">
											<items jcr:primaryType="nt:unstructured">
												<item jcr:primaryType="nt:unstructured"
													  sling:resourceType="granite/ui/components/coral/foundation/well"
													  jcr:title="Item Card">
													<items jcr:primaryType="nt:unstructured">
														<iconFlame
																jcr:primaryType="nt:unstructured"
																sling:resourceType="granite/ui/components/foundation/include"
																path="/apps/santander-spa-first/components/reusable/dialog/cq:dialog/content/items/iconFlame"/>
														<primaryText
																jcr:primaryType="nt:unstructured"
																sling:resourceType="granite/ui/components/foundation/include"
																path="/apps/santander-spa-first/components/reusable/dialog/cq:dialog/content/items/primaryText"/>
													</items>
												</item>
											</items>
										</item>
									</items>
								</field>
							</multifield>
						</items>
					</properties>
				</items>
			</tabs>
		</items>
	</content>
</jcr:root>